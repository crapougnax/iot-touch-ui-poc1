{
  "name": "touch.poc",
  "version": "1.0.0",
  "description": "A simple POC to test touch screen on RPi",
  "main": "boot.js",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress --config webpack/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "nodemon src/boot.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-3"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier",
      "plugin:import/errors"
    ],
    "plugins": [
      "prettier",
      "import"
    ],
    "parser": "babel-eslint",
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "arrowParens": "always",
          "singleQuote": true,
          "trailingComma": "es5",
          "semi": false
        }
      ],
      "no-irregular-whitespace": 0
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    }
  },
  "author": "Olivier LÃ©pine",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "bootstrap": "^3.3.7",
    "chalk": "^2.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint-loader": "^2.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "hard-source-webpack-plugin": "^0.6.4",
    "image-webpack-loader": "^4.2.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.3",
    "pug": "^2.0.3",
    "style-loader": "^0.20.3",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.4.1",
    "webpack-assets-manifest": "^3.0.0",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "webpack-cli": "^2.0.13",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.4.2",
    "nock": "^9.2.3",
    "nodemon": "^1.17.2",
    "prettier-eslint": "^8.8.1"
  }
}
